server {
    listen      80;
    #server_name "~^(?!www\.).*";
    server_name fantasydata.co;
    return      301 $scheme://www.$host$request_uri;
}

server {
    listen       80;
    server_name  www.fantasydata.co;

    location / {
        proxy_pass http://127.0.0.1:8080;
    }
}

server {
    listen       80;
    server_name  local.app.fantasydata.co;

    location / {
        proxy_pass http://127.0.0.1:8080;
    }
}
